{
  "$schema": "https://titan.apimatic.io/api/build/schema",
  "buildFileVersion": "1.0",
  "generatePortal": {
    "pageTitle": "ThoughtSpot Public Rest APIs",
    "languageConfig": {
      "http": {}
    },
    "headIncludes": "<link rel=\"icon\" type=\"image/svg+xml\" href='static/images/favicon.svg' /><link rel=\"stylesheet\" href=\"./static/styles/overrides.css\">",
    "tailIncludes": "<script id=\"ts-playground-snippet\" src=\"./static/js/embedded.js\"></script>",
    "portalSettings": {
      "themeOverrides": {
        "themeType": "cool",
        "palette": {
          "primaryColor": "#323946",
          "linkColor": "#2770EF",
          "colors": {
            "C000": "#FFFFFF",
            "C100": "#F9FAFE",
            "C200": "#FFFFFF",
            "C300": "#C0C6CF",
            "C400": "#777e8b",
            "C500": "#B0B5C9",
            "C600": "#1d232f",
            "C700": "#606E8A",
            "C800": "#1d232f",
            "C900": "#1C2029",
            "C1000": "#1D2236"
          }
        },
        "fontSource": [],
        "cssStyles": {
          "headings": {
            "fontFamily": "Optimo-Plain",
            "h1": {
              "fontFamily": "Optimo-Plain",
              "fontSize": "28px",
              "fontWeight": "700",
              "fontStyle": "normal",
              "lineHeight": "1"
            },
            "h2": {
              "fontFamily": "Optimo-Plain",
              "fontSize": "26px",
              "fontWeight": "300",
              "fontStyle": "normal",
              "lineHeight": "1"
            },
            "h3": {
              "fontFamily": "Optimo-Plain",
