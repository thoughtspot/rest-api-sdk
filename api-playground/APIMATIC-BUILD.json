{
    "$schema": "https://titan.apimatic.io/api/build/schema",
    "buildFileVersion": "1.0",
    "generatePortal": {
        "pageTitle": "ThoughtSpot Public Rest APIs",
        "navTitle": "Public Rest APIs",
        "logoUrl": "static/images/ts-logo-white.svg",
        "logoLink": "https://www.thoughtspot.com",
        "languageConfig": {
            "http": {}
        },
        "headIncludes": "<link rel=\"icon\" type=\"image/svg+xml\" href='static/images/favicon.svg' /><link rel=\"stylesheet\" href=\"static\/css\/overrides.css\">",
        "tailIncludes": "<script id=\"ts-playground-snippet\" src=\".\/static\/js\/embedded.js\"><\/script>",
        "portalSettings": {
            "themeOverrides": {
                "themeType": "cool",
                "palette": {
                    "primaryColor": "#323946",
                    "linkColor": "#2770EF",
                    "colors": {
                        "C000": "#FFFFFF",
                        "C200": "#FFFFFF",
                        "C300": "#C0C6CF",
                        "C400": "#777e8b",
                        "C600": "#1d232f",
                        "C700": "#606E8A",
                        "C800": "#1d232f",
                        "C900": "#1C2029"
                    }
                },
                "fontSource": [],
                "cssStyles": {
                    "headings": {
                        "fontFamily": "Optimo-Plain",
                        "h1": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "28px",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "lineHeight": "1"
                        },
                        "h2": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "26px",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "lineHeight": "1"
                        },
                        "h3": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "23px",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "lineHeight": "1"
                        },
                        "h4": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "20px",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "lineHeight": "1"
                        },
                        "h5": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "18px",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "lineHeight": "1"
                        }
                    },
                    "body": {
                        "fontFamily": "Optimo-Plain",
                        "text1": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "14.5px",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "lineHeight": "1.6rem"
                        },
                        "text2": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "14.5px",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "lineHeight": "1.6rem"
                        },
                        "text3": {
                            "fontFamily": "Optimo-Plain",
                            "fontSize": "14.5px",
                            "fontWeight": "300",
                            "fontStyle": "normal",
                            "lineHeight": "1.6rem"
                        }
                    },
                    "code": {
                        "fontFamily": "Roboto,Consolas,Monaco,monospace",
                        "blockCode": {
                            "fontFamily": "Roboto,Consolas,Monaco,monospace",
                            "fontSize": "13.5px",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "lineHeight": "1.4rem"
                        },
                        "inlineCode": {
                            "fontFamily": "Roboto,Consolas,Monaco,monospace",
                            "fontSize": "13.5px",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "lineHeight": "1.4rem"
                        }
                    }
                }
            },
            "enableExport": false,
            "enableConsoleCalls": true
        }
    }
}