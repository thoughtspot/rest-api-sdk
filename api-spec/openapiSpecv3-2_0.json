    - name: "DataPipeline"
      description: "DataPipeline Services"
    - name: "DBT"
      description: "DBT Services"
    - name: "Email Customisation"
      description: "Coms Email Customisation Configuration"
    - name: "Collections"
      description: "Collection Management Services"
    - name: "Groups"
      description: "Group Management Services"
    - name: "Link Customizations"
      description: "Link Customizations Settings"
