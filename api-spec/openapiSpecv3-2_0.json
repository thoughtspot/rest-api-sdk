# OpenAPI's specification version
openapi: "3.0.3"

# Version and Description of the API specification
info:
    title: "ThoughtSpot Callosum Service REST API"
    description: "ThoughtSpot REST API of Callosum Service to manage and access ThoughtSpot Objects"
    version: "2.0.2"
    termsOfService: "https://www.thoughtspot.com/eula" # https://www.thoughtspot.com/thoughtspot-website-terms-use
    contact:
        email: "support@thoughtspot.com"
    license:
        name: "Apache 2.0"
        url: "https://www.apache.org/licenses/LICENSE-2.0.html"

# Server details with base URL info
servers:
    - url: "{protocol}://{host}:{port}/callosum/v1/v2"
      variables:
          protocol:
              description: "ThoughtSpot cluster HTTP protocol"
              enum:
                  - "https"
                  - "http"
              default: "https"
          host:
              description: "ThoughtSpot cluster IP address or FQDN"
              default: "champagne.thoughtspotstaging.cloud"
          port:
              description: "ThoughtSpot cluster port address"
              enum:
                  - "443"
                  - "8088"
              default: "443"

# Custom properties definitions
x-thoughtspot-allowed-roles:
    $ref: "#/components/schemas/Privileges"

x-thoughtspot-allowed-permissions:
    $ref: "#/components/schemas/Permissions"

x-thoughtspot-allowed-scopes:
    $ref: "#/components/schemas/Scopes"

x-thoughtspot-private-api-release-version:
    $ref: "#/components/schemas/PrivateReleaseVersions"

x-thoughtspot-public-api-release-version:
    $ref: "#/components/schemas/PublicReleaseVersions"

# URL to external documentation
externalDocs:
    description: "ThoughtSpot Callosum Service REST API v2 Beta Documentation"
    url: "https://developers.thoughtspot.com/docs/?pageid=rest-api-v2"

