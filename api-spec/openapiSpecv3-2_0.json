            requestBody:
                description: "User details"
                required: true
                content:
                    application/x-www-form-urlencoded:
                        schema:
                            type: object
                            required:
                                - name
                                - display_name
                                - email
                            properties:
                                name:
                                    description: "Unique name of the user."
                                    type: string
                                display_name:
                                    description: "Unique display name of the user."
                                    type: string
                                password:
                                    description: "Password of the user."
                                    type: string
                                account_type:
                                    description: "Type of the user account."
                                    allOf:
                                        - $ref: "#/components/schemas/User_Account_Type"
                                    default: LOCAL_USER
                                account_status:
                                    description: "Status of the user account."
                                    allOf:
                                        - $ref: "#/components/schemas/User_Account_Status_Type"
                                    default: ACTIVE
                                email:
                                    description: "Email of the user."
                                    type: string
                                org_identifiers:
                                    description: "Unique ID or name of the Orgs for the users."
                                    type: array
                                    items:
                                        type: string
                                group_identifiers:
                                    description: "Unique ID or name of the User Groups of the users."
                                    type: array
                                    items:
                                        type: string
                                visibility:
                                    description: "Visibility of the users. The SHARABLE makes a users visible to other users and user groups, and thus allows them to share objects."
                                    allOf:
                                        - $ref: "#/components/schemas/User_Account_Visibility_Type"
                                    default: SHARABLE
                                notify_on_share:
                                    description: "User preference for receiving email notifications on shared answers or liveboard."
                                    type: boolean
                                    default: true
                                show_onboarding_experience:
                                    description: "The user preference for revisiting the onboarding experience."
                                    type: boolean
                                    default: true
                                onboarding_experience_completed:
                                    description: "The user preference for turning off the onboarding experience."
                                    type: boolean
                                    default: false
                                home_liveboard_identifier:
                                    description: "Unique ID or name of the users home liveboard."
                                    type: string
                                favorite_metadata:
                                    description: "Metadata objects to be assigned as favorites of the user."
                                    type: array
                                    items:
                                        type: object
                                        required:
                                            - identifier
                                        properties:
                                            identifier:
                                                description: "Unique ID or name of the metadata."
                                                type: string
                                            type:
                                                description: "Type of metadata."
                                                allOf:
                                                    - $ref: "#/components/schemas/User_Favorite_Metadata_Type"
                                preferred_locale:
                                    description: "Locale for the user."
                                    type: string
                                use_browser_language:
                                    x-thoughtspot-public-api-release-version:
                                        - 26.3.0.cl
                                    x-thoughtspot-private-api-release-version:
                                        - 26.3.0.cl
                                    description: "Flag to indicate whether to use the browser locale for the user in the UI. When set to true, the preferred_locale value is unset and the browser's language setting takes precedence."
                                    type: boolean
                                    default: false
                                extended_properties:
                                    description: "Properties for the user"
                                    type: object
                                extended_preferences:
                                    description: "Preferences for the user"
                                    type: object
                                trigger_welcome_email:
                                    description: "Flag to indicate whether welcome email should be sent to user"
                                    type: boolean
                                trigger_activation_email:
                                    description: "Flag to indicate whether activation email should be sent to user"
                                    type: boolean

            responses:
                "200":
                    description: "User successfully created."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Response_User_Data"
                "400":
                    description: "Invalid request."
                    content:
