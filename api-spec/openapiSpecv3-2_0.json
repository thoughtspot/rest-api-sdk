                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_500"

    /auth/session/login:
        post:
            tags:
                - Authentication
            summary: "Login using either username, password or bearer token"
            operationId: login-session
            security:
                - Security_Bearer_Token: []
                - Security_Cookie: []
            x-thoughtspot-public-api-release-version:
                - 9.0.0.cl
            x-thoughtspot-private-api-release-version:
