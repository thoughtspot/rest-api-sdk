        Response_Roles_Data:
            type: object
            required:
                - data
            properties:
                data:
                    type: array
                    items:
                        $ref: "#/components/schemas/Response_Role"

        Response_Role_Data:
            type: object
            required:
                - data
            properties:
                data:
                    $ref: "#/components/schemas/Response_Role"

        Response_Role:
            type: object
            description: "Role Details"
            required:
                - id
                - name
                - description
                - privileges
            properties:
                id:
                    description: "Unique Id of the role."
                    allOf:
                        - $ref: "#/components/schemas/Unique_Identifier"
                name:
                    description: "Name of the role"
                    type: string
                description:
                    description: "Description of the role"
                    type: string
                privileges:
                    type: array
                    description: "Privileges granted to the role."
                    items:
                        $ref: "#/components/schemas/Role_Privileges"
                groups_assigned_count:
                    description: "number of groups assigned with this role"
                    type: integer
                orgs:
                    description: "Organizations in which role exists."
                    type: array
                    items:
                        $ref: "#/components/schemas/Object_ID_And_Name"
                groups:
                    description: "Details of groups assigned with this role "
                    type: array
                    items:
                        $ref: "#/components/schemas/Role_User_Group"
                permissions:
                    description: "Permission details of the Role"
                    allOf:
                        - $ref: "#/components/schemas/Permission_Type"
                author_id:
                    description: "Unique identifier of author of the role."
                    allOf:
                        - $ref: "#/components/schemas/Unique_Identifier"
                modifier_id:
                    description: "Unique identifier of modifier of the role."
                    allOf:
                        - $ref: "#/components/schemas/Unique_Identifier"
                creation_time_in_millis:
                    description: "Creation time of the role in milliseconds."
                    type: integer
                modification_time_in_millis:
                    description: "Last modified time of the role in milliseconds."
                    type: integer
                deleted:
                    description: "Indicates whether the role is deleted."
                    type: boolean
                hidden:
                    description: "Indicates whether the role is hidden."
                    type: boolean
                external:
                    description: "Indicates whether the role is external."
                    type: boolean
                deprecated:
                    description: "Indicates whether the role is deprecated."
                    type: boolean
                shared_via_connection:
                    description: "Indicates whether the role is shared via connection"
                    type: boolean

        Role_User_Group:
            description: "User Group Details"
            type: object
            required:
                - id
                - name
            properties:
                id:
                    description: "Unique identifier of the user group."
                    allOf:
                        - $ref: "#/components/schemas/Unique_Identifier"
                name:
                    description: "Name of the user group."
                    type: string

        Custom_Action_Details:
            type: object
            description: "Custom action type and configuration details."
            properties:
                CALLBACK :
                    description: "CALLBACK Custom Action Type"
                    type: object
                    properties:
                        reference:
                            type: string
                            description: "Reference name of the sdk. By default the value will be set to action name"
                URL:
                    description: "URL Custom Action Type"
                    type: object
                    required:
                        - url
                    properties:
                        url:
                            type: string
                            description: "Request Url for the Custom action."
                        reference:
                            type: string
                            description: "Reference name of the sdk. By default the value will be set to action name"
                        authentication:
                            type: object
                            description: "Authorization type for the custom action."
                            allOf:
                                - $ref: "#/components/schemas/Custom_Action_Auth_Type"
                        parameters:
                            description: "Query parameters for url."
                            type: array
                            items:
                                type: object
                                properties:
                                    key:
                                        description: "Key for the url query parameter"
                                        type: string
                                    value:
                                        description: "Value for the url query parameter"
                                        type: string

        Custom_Action_Auth_Type:
            description: "Configuration for the Authorization type of the custom action."
            allOf:
                - $ref: "#/components/schemas/Custom_Action_Auth_Type_Configurations"

        Custom_Action_Auth_Type_Configurations:
            type: object
            description: "Authorization Configurations for different Authorization Types."
            properties:
                No_Auth:
                    description: "No authorization. If your request doesn't require authorization."
                    type: string
                API_Key:
                    description: "With API key auth, you send a key-value pair to the API either in the request headers or query parameters."
                    type: object
                    properties:
                        key:
                            description: "Enter your key name"
                            type: string
                        value:
                            description: "Enter you key value"
                            type: string
                Basic_Auth:
                    description: "Basic Auth: Basic authentication involves sending a verified username and password with your request."
                    type: object
                    properties:
                        username:
                            description: "Username for the basic authentication"
                            type: string
                        password:
                            description: "Password for the basic authentication"
                            type: string
                Bearer_Token:
                    description: "Bearer tokens enable requests to authenticate using an access key."
                    type: string

        Response_Custom_Actions_Data:
            type: object
            required:
                - data
            properties:
                data:
                    type: array
                    items:
                        $ref: "#/components/schemas/Response_Custom_Action"

        Response_Custom_Action_Data:
            type: object
            required:
                - data
            properties:
                data:
                    $ref: "#/components/schemas/Response_Custom_Action"

        Response_Custom_Action:
            type: object
            description: "Custom action details"
            required:
                - id
                - name
                - action_details
                - default_action_config
            properties:
                id:
                    description: "Unique Id of the custom action."
                    allOf:
                        - $ref: "#/components/schemas/Unique_Identifier"
                name:
                    description: "Unique name of the custom action."
                    type: string
                action_details:
                    description: "Type and Configuration for Custom Actions"
                    type: object
                    allOf:
                        - $ref: "#/components/schemas/Custom_Action_Details"
                default_action_config:
                    description: "Default Custom action configuration. This includes if the custom action visibility across visualization. By default, visible across visualizations."
                    type: object
                    properties:
                        visibility:
                            description: "Custom action is visible to all Visualizations. Earlier naming convention: LOCAL/GLOBAL. TRUE signifies GLOBAL for backward compatibility."
                            type: boolean
                user_groups:
                    description: "Unique ID or name of the User groups which are associated with the custom action."
                    type: array
                    items:
                        $ref: "#/components/schemas/Object_ID_And_Name"
                metadata_association:
                    description: "Metadata objects to which the custom action needs to be associated."
                    type: array
                    items:
                        type: object
                        required:
                            - identifier
                            - type
                            - action_config
                        properties:
                            identifier:
                                description: "Unique ID or name of the metadata."
                                type: string
                            type:
                                description: "Type of metadata."
                                allOf:
                                    - $ref: "#/components/schemas/Custom_Action_Association_Metadata_Type"
                            action_config:
                                description: "Specify that the association is enabled for the metadata object"
                                type: object
                                properties:
                                    visibility:
                                        description: "Visibility of the metadata association with custom action. Earlier naming convention: enabled"
                                        type: boolean
                                    position:
                                        description: "Position of the Custom action on the Metadata object. Earlier naming convention: context."
                                        allOf:
                                            - $ref: "#/components/schemas/Custom_Action_Context_Type"
                                        type: string
