                                    description: "The number of records that should be included."
                                    type: integer
                                    default: 10
                                tag_identifiers:
                                    description: "Unique ID or name of tags."
                                    type: array
                                    items:
                                        type: string
                                created_by_user_identifiers:
                                    description: "Unique ID or name of users who has created the metadata object."
                                    type: array
                                    items:
                                        type: string
                                modified_by_user_identifiers:
                                    description: "Unique ID or name of users who has modified the metadata object."
                                    type: array
                                    items:
                                        type: string
                                exclude_objects:
                                    description: "Exclude Metadata objects."
                                    type: array
                                    items:
                                        type: object
                                        required:
                                            - identifier
                                            - type
                                        properties:
                                            identifier:
                                                description: "Unique ID or name of the metadata."
                                                type: string
                                            type:
                                                description: "Type of metadata."
                                                allOf:
                                                    - $ref: "#/components/schemas/Search_Metadata_Type"
                                include_headers:
                                    description: "Indicates whether to include headers of the metadata objects."
                                    type: boolean
                                    default: true
                                include_details:
                                    description: "Indicates whether to include complete details of the metadata objects."
                                    type: boolean
                                    default: false
                                include_stats:
                                    description: "Indicates whether to include stats of the metadata objects."
                                    type: boolean
                                    default: false
                                include_visualization_headers:
                                    description: "Indicates whether to include visualization headers of the Liveboard metadata objects."
                                    type: boolean
                                    default: false
                                include_hidden_objects:
                                    description: "Indicates whether to include hidden metadata objects."
                                    type: boolean
                                    default: false
                                include_dependent_objects:
                                    description: "Indicates whether to include dependent objects of the metadata objects."
                                    type: boolean
                                    default: false
                                dependent_objects_record_size:
                                    x-thoughtspot-public-api-release-version:
                                        - 9.12.5.cl
                                    x-thoughtspot-private-api-release-version:
                                        - 9.12.5.cl
                                    description: "The maximum number of dependents to include per metadata object."
                                    type: integer
                                    default: 0
                                include_incomplete_objects:
                                    description: "Indicates whether to include details of incomplete information of the metadata objects if any present."
                                    type: boolean
                                    default: false
                                include_auto_created_objects:
                                    description: "Indicates whether to include auto created metadata objects."
                                    type: boolean
                                    default: false
                                include_worksheet_search_assist_data:
                                    description: "Indicates whether to include search assist data of the worksheet objects."
                                    type: boolean
                                    default: false
                                include_knowledge_card_headers:
                                    x-thoughtspot-public-api-release-version:
                                        - NA
                                    x-thoughtspot-private-api-release-version:
                                        - 10.2.0.cl
                                    description: "Indicates whether to include details of knowledge_card_headers information of the Logical table."
                                    type: boolean
                                    default: false
                                include_discoverable_objects:
                                    x-thoughtspot-public-api-release-version:
                                        - 10.7.0.cl
                                    x-thoughtspot-private-api-release-version:
                                        - 10.7.0.cl
                                    description: "Indicates whether to include discoverable objects."
                                    type: boolean
                                    default: true
                                sort_options:
                                    description: "Sort options."
                                    type: object
                                    properties:
                                        field_name:
                                            description: "Name of the field to apply the sort on."
                                            allOf:
                                                - $ref: "#/components/schemas/Metadata_Search_Field_Name"
                                        order:
                                            description: "Sort order : ASC(Ascending) or DESC(Descending)."
                                            allOf:
                                                - $ref: "#/components/schemas/Sort_Order"
                                permissions:
                                    description: "Permission details for searching the objects."
                                    type: array
                                    items:
                                        type: object
                                        required:
                                            - principal
                                            - share_mode
                                        properties:
                                            principal:
                                                description: "Details of users or user groups."
                                                type: object
                                                required:
                                                    - type
                                                    - identifier
                                                properties:
                                                    type:
                                                        description: "Principal type."
                                                        allOf:
                                                            - $ref: "#/components/schemas/Principal_Type"
                                                    identifier:
                                                        description: "Unique ID or name of the principal."
                                                        type: string
                                            share_mode:
                                                description: "Object share mode."
                                                allOf:
                                                    - $ref: "#/components/schemas/Permission_Type"
                                favorite_object_options:
                                    description: "Favorite object options."
                                    type: object
                                    properties:
                                        include:
                                            description: "Only include object marked as favorite."
                                            type: boolean
                                            default: false
                                        user_identifiers:
                                            description: "Unique ID or name of the users. If not specified then favorite of current logged in user is considered."
                                            type: array
                                            items:
                                                type: string
                                show_resolved_parameters:
                                    x-thoughtspot-public-api-release-version:
                                        - 10.9.0.cl
                                    x-thoughtspot-private-api-release-version:
                                        - 10.9.0.cl
                                    description: "Indicates whether to show resolved parameter values in the metadata objects."
                                    type: boolean
                                liveboard_response_version:
                                    x-thoughtspot-public-api-release-version:
                                        - 10.10.0.cl
                                    x-thoughtspot-private-api-release-version:
                                        - 10.10.0.cl
                                    description: "Version of the liveboard to be attached in metadata detail."
                                    allOf:
                                        - $ref: "#/components/schemas/Liveboard_Model_Version"
                                    default: V1
                                include_only_published_objects:
                                    x-thoughtspot-public-api-release-version:
                                        - 10.11.0.cl
                                    x-thoughtspot-private-api-release-version:
                                        - 10.11.0.cl
                                    description: "Indicates whether only published objects be fetched."
                                    type: boolean
                                    default: false
            responses:
                "200":
                    description: "Metadata objects search result."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Response_Metadata_Search_Data"
                "400":
                    description: "Invalid request."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_400"
                "401":
                    description: "Unauthorized access."
                    content:
                        text/html:
                            schema:
                                type: string
                "403":
                    description: "Forbidden access."
                    content:
                        text/html:
                            schema:
                                type: string
                "500":
                    description: "Unexpected error"
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_500"

