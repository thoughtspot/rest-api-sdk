# Definition of each API
paths:
    /auth/token:
        post:
            tags:
                - Authentication
            summary: "create bearer auth token"
            operationId: create-bearer-auth-token
            security:
                - Security_Bearer_Token: []
                - Security_Cookie: []
            x-thoughtspot-public-api-release-version:
                - 9.0.0.cl
            x-thoughtspot-private-api-release-version:
                - 9.0.0.cl
            requestBody:
                description: "Bearer auth token creation details"
                required: true
                content:
                    application/x-www-form-urlencoded:
                        schema:
                            type: object
                            required:
                                - username
                                - credential
                                - scope
                            properties:
                                username:
                                    description: "Username for whom the token need to created."
                                    type: string
                                credential:
                                    description: "User credential details."
                                    type: object
                                    required:
                                        - type
                                        - value
                                    properties:
                                        type:
                                            description: "Type of credential."
                                            allOf:
                                                - $ref: "#/components/schemas/Auth_Token_Credential_Type"
                                        value:
                                            description: "Value of the chosen credential type."
                                            type: string
                                scope:
                                    description: "Token access scope details."
                                    type: object
                                    properties:
                                        access_type:
                                            description: "Object access scope type."
                                            allOf:
                                                - $ref: "#/components/schemas/Auth_Token_Scope_Type"
                                        metadata_id:
                                            description: "Unique identifier of the metadata."
                                            type: string
                                        org_id:
                                            description: "Unique identifier of the organization."
                                            type: integer
                                user_options:
                                    description: "User options to create user if one does not exist. Use this parameter to provision a user just-in-time (JIT)."
                                    type: object
                                    properties:
                                        email:
                                            x-thoughtspot-public-api-release-version:
                                                - 9.1.0.cl
                                            x-thoughtspot-private-api-release-version:
                                                - 9.1.0.cl
                                            description: "Indicates email id of the user. Use this parameter to provision a user just-in-time (JIT)."
                                            type: string
                                        display_name:
                                            x-thoughtspot-public-api-release-version:
                                                - 9.1.0.cl
                                            x-thoughtspot-private-api-release-version:
                                                - 9.1.0.cl
                                            description: "Indicates display name of the user. Use this parameter to provision a user just-in-time (JIT)."
                                            type: string
                                        auto_create:
                                            description: "Indicates whether to auto create user if one does not exist. Use this parameter to provision a user just-in-time (JIT)."
                                            type: boolean
                                            default: false
                                        group_identifiers:
                                            description: "Unique ID or name of the User Groups to which newly created user to be added. Use this parameter to provision a user just-in-time (JIT)."
                                            type: array
                                            items:
                                                type: string
                                jwt_user_options:
                                    description: "JWT user options to create a JWT token given the payload."
                                    type: object
                                    deprecated: true
                                    properties:
                                        parameters:
                                            description: "List of runtime parameters need to set during the session."
                                            type: array
                                            items:
                                                anyOf:
                                                    - $ref: "#/components/schemas/JWT_PARAMETER_TYPE_RUNTIME_FILTER"
                                                    - $ref: "#/components/schemas/JWT_PARAMETER_TYPE_RUNTIME_SORT"
                                                    - $ref: "#/components/schemas/JWT_PARAMETER_TYPE_RUNTIME_PARAM_OVERRIDE"
                                        metadata:
                                            description: "Metadata objects."
                                            type: array
                                            items:
                                                type: object
                                                properties:
                                                    identifier:
                                                        description: "Unique ID or name of the metadata."
                                                        type: string
                                                    type:
                                                        description: "Type of metadata."
                                                        allOf:
                                                            - $ref: "#/components/schemas/JWT_Metadata_Type"
                                user_parameters:
                                    description: "Configuration for user-defined parameters and runtime settings."
                                    x-thoughtspot-public-api-release-version:
                                        - 9.10.5.cl
                                    x-thoughtspot-private-api-release-version:
                                        - 9.10.5.cl
                                    type: object
                                    properties:
                                        objects:
                                            description: "Objects to which parameters will be applied."
                                            required:
                                                - identifier
                                            type: array
                                            items:
                                                type: object
                                                properties:
                                                    type:
                                                        description: "Type of object."
                                                        allOf:
                                                            - $ref: "#/components/schemas/USER_PARAMETERS_OBJECTS"
                                                    identifier:
                                                        description: "Unique name/id of the object."
                                                        type: string
                                        runtime_filters:
                                            description: "Runtime filters to apply on data."
                                            type: array
                                            items:
                                                type: object
                                                required:
                                                    - column_name
                                                    - operator
                                                    - values
                                                properties:
                                                    column_name:
                                                        description: "The column name to apply the filter."
                                                        type: string
                                                    values:
                                                        description: "Values to filter on."
                                                        type: array
                                                        items:
                                                            type: string
                                                    operator:
                                                        description: "Filter operator. Example: EQ (equals), GT (greater than), etc."
                                                        allOf:
                                                            - $ref: "#/components/schemas/RUNTIME_FILTER_OPERATORS"
                                                    persist:
                                                        description: "Flag to persist the runtime filters."
                                                        type: boolean
                                                        default: false
                                                        x-thoughtspot-public-api-release-version:
                                                            - 9.12.0.cl
                                                        x-thoughtspot-private-api-release-version:
                                                            - 9.12.0.cl
                                                    objects:
                                                        description: "Objects to which the filter applies."
                                                        required:
                                                            - identifier
                                                        type: array
                                                        items:
                                                            type: object
                                                            properties:
                                                                type:
                                                                    description: "Type of object."
                                                                    allOf:
                                                                        - $ref: "#/components/schemas/USER_PARAMETERS_OBJECTS"
                                                                identifier:
                                                                    description: "Unique name/id of the object."
                                                                    type: string
                                        runtime_sorts:
                                            description: "Runtime sorts to apply on data."
                                            type: array
                                            items:
                                                type: object
                                                properties:
                                                    column_name:
                                                        description: "The column name to apply the sort."
                                                        type: string
                                                    order:
                                                        description: "Sort order (ASC or DESC)."
                                                        default: "ASC"
                                                        allOf:
                                                            - $ref: "#/components/schemas/Sort_Order"
                                                    persist:
                                                        description: "Flag to persist the runtime sorts."
                                                        type: boolean
                                                        default: false
                                                        x-thoughtspot-public-api-release-version:
                                                            - 9.12.0.cl
                                                        x-thoughtspot-private-api-release-version:
                                                            - 9.12.0.cl
                                                    objects:
                                                        description: "Objects to which the sort applies."
                                                        type: array
                                                        required:
                                                            - identifier
                                                        items:
                                                            type: object
                                                            properties:
                                                                type:
                                                                    description: "Type of object."
                                                                    allOf:
                                                                        - $ref: "#/components/schemas/USER_PARAMETERS_OBJECTS"
                                                                identifier:
                                                                    description: "Unique name/id of the object."
                                                                    type: string
                                        parameters:
                                            description: "Parameters overrides"
                                            type: array
                                            items:
                                                type: object
                                                required:
                                                    - name
                                                    - values
                                                properties:
                                                    name:
                                                        description: "Parameter name."
                                                        type: string
                                                    values:
                                                        description: "Possible values for the parameter."
                                                        type: array
                                                        items:
                                                            type: string
                                                    persist:
                                                        description: "Flag to persist the parameters."
                                                        type: boolean
                                                        default: false
                                                        x-thoughtspot-public-api-release-version:
                                                            - 9.12.0.cl
                                                        x-thoughtspot-private-api-release-version:
                                                            - 9.12.0.cl
                                                    objects:
                                                        description: "Objects to which the parameter applies."
                                                        required:
                                                            - identifier
                                                        type: array
                                                        items:
                                                            type: object
                                                            properties:
                                                                type:
                                                                    description: "Type of object."
                                                                    allOf:
                                                                        - $ref: "#/components/schemas/USER_PARAMETERS_OBJECTS"
                                                                identifier:
                                                                    description: "Unique name/id of the object."
                                                                    type: string
                                        compress:
                                            description: "Flag indicating whether the generated token should be compressed."
                                            x-thoughtspot-public-api-release-version:
                                                - 9.12.0.cl
                                            x-thoughtspot-private-api-release-version:
                                                - 9.12.0.cl
                                            type: boolean
                                            default: false
                                validity_time_in_sec:
                                    description: "Token validity time in seconds."
                                    type: integer
                                    default: 300
            responses:
                "200":
                    description: "Bearer auth token creation successful."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Response_Bearer_Auth_Token_Data"
                "400":
                    description: "Invalid request."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_400"
                "401":
                    description: "Unauthorized access."
                    content:
                        text/html:
                            schema:
                                type: string
                "403":
                    description: "Forbidden access."
                    content:
                        text/html:
                            schema:
                                type: string
                "500":
                    description: "Unexpected error"
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_500"

    /auth/token/access-control:
        post:
            tags:
                - Authentication
            summary: "Create a JWT bearer authentication token with Attribute-Based Access Control (ABAC)."
            operationId: create-abac-jwt-auth-token
            security:
                - Security_Bearer_Token: [ ]
                - Security_Cookie: [ ]
            x-thoughtspot-public-api-release-version:
                - 10.7.0.cl
            x-thoughtspot-private-api-release-version:
                - 10.7.0.cl
            requestBody:
                description: "Details required to create a JWT bearer authentication token."
                required: true
                content:
                    application/x-www-form-urlencoded:
                        schema:
                            type: object
                            required:
                                - username
                                - credential
                                - persist_option
                            properties:
                                username:
                                    description: "The username for which the token needs to be created."
                                    type: string
                                credential:
                                    description: "User credential details. User needs to select either Secret Key or Password"
                                    type: object
                                    required:
                                        - type
                                        - value
                                    properties:
                                        type:
                                            description: "Type of credential."
                                            anyOf:
                                                - $ref: "#/components/schemas/Auth_ABAC_Token_Credential_Type"
                                        value:
                                            description: "Value of the chosen credential type."
                                            type: string
                                org_identifier:
                                    description: "The unique identifier or name of the organization."
                                    type: string
                                persist_option:
                                    description: "Indicates whether the specified attributes should be persisted or not."
                                    type: string
                                    anyOf:
                                        - $ref: "#/components/schemas/PERSIST_OPTION"
                                reset_option:
                                    description: "Indicates which all fields should be reset. To be used when persist_option is RESET."
                                    type: array
                                    items:
                                        type: string
                                        anyOf:
                                            - $ref: "#/components/schemas/RESET_OPTION"
                                    uniqueItems: true
                                runtime_filters:
                                    description: "Filters to apply on data at runtime."
                                    type: array
                                    items:
                                        type: object
                                        required:
                                            - column_name
                                            - operator
                                            - values
                                        properties:
                                            column_name:
                                                description: "The name of the column to apply the filter on."
                                                type: string
                                            values:
                                                description: "The values to filter on."
                                                type: array
                                                items:
                                                    type: string
                                            operator:
                                                description: "The operator to use for filtering. Example: EQ (equals), GT (greater than), etc."
                                                allOf:
                                                    - $ref: "#/components/schemas/RUNTIME_FILTER_OPERATORS"
                                            objects:
                                                $ref: "#/components/schemas/Objects"
                                override_parameters:
                                    description: "Parameters to override."
                                    type: array
                                    items:
                                        type: object
                                        required:
                                            - name
                                            - values
                                        properties:
                                            name:
                                                description: "The name of the parameter."
                                                type: string
                                            values:
                                                description: "The values for the parameter. Only single value is supported for Parameter Override"
                                                type: array
                                                items:
                                                    type: string
                                            objects:
                                                $ref: "#/components/schemas/Objects"
                                variable_values:
                                    description: "Formula Variable which can be used in RLS/Formula creation"
                                    type: array
                                    items:
                                        type: object
                                        required:
                                            - name
                                            - values
                                        properties:
                                            name:
                                                description: "The name of the formula variable."
                                                type: string
                                            values:
                                                description: "The values for the formula variable."
                                                type: array
                                                items:
                                                    type: string
                                objects:
                                    $ref: "#/components/schemas/Objects"
                                display_name:
                                    description: "(just-in-time (JIT) provisioning) Indicates display name of the user. Specify this attribute when creating a new user."
                                    type: string
                                email:
                                    description: "(just-in-time (JIT) provisioning)Email address of the user. Specify this attribute when creating a new user. In case of IAM V2, Email is mandatory"
                                    type: string
                                groups:
                                    description: "(just-in-time (JIT) provisioning) ID or name of the groups to which the newly created user belongs. Specify this attribute when creating a new user."
                                    type: array
                                    items:
                                        type: object
                                        properties:
                                            identifier:
                                                description: "The unique identifier or name of the group."
                                                type: string
                                validity_time_in_sec:
                                    description: "The validity time of the token in seconds."
                                    type: integer
                                    default: 300
                                auto_create:
                                    description: "(just-in-time (JIT) provisioning) Indicates whether to auto create user if one does not exist."
                                    type: boolean
                                    default: true
            responses:
                "200":
                    description: "ABAC token creation was successful."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Response_ABAC_Auth_Token_Data"
                "400":
                    description: "Invalid request. This could be due to missing or incorrect parameters."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_400"
                "401":
                    description: "Unauthorized access. The request could not be authenticated."
                    content:
                        text/html:
                            schema:
                                type: string
                "403":
                    description: "Forbidden access. The user does not have permission to access this resource."
                    content:
                        text/html:
                            schema:
                                type: string
                "500":
                    description: "An unexpected error occurred on the server."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_500"

    /cluster/license:
        put:
            tags:
                - Cluster
            summary: "Enable or Disable TSE license Per Org"
            operationId: cluster-license-configuration
            security:
                - Security_Bearer_Token: [ ]
                - Security_Cookie: [ ]
            x-thoughtspot-allowed-roles:
                - ADMINISTRATION
            x-thoughtspot-private-api-release-version:
                - 9.7.0.cl
            requestBody:
                description: "TSE License configuration details"
                required: true
                content:
                    application/x-www-form-urlencoded:
                        schema:
                            type: object
                            required:
                                - configuration
                            properties:
                                configuration:
                                    description: "List of License Configurations for Orgs"
                                    type: array
                                    items:
                                        type: object
                                        required:
                                            - org_identifier
                                            - license
                                        properties:
                                            org_identifier:
                                                description: "Unique ID or name of the organization."
                                                type: string
                                            license:
                                                description: "Flag to enable or disable TSE License."
                                                type: array
                                                items:
                                                    type: object
                                                    required:
                                                        - name
                                                        - enable
                                                    properties:
                                                        name:
                                                            description: "License Service"
                                                            allOf:
                                                                - $ref: "#/components/schemas/License_Type"
                                                            default: TSE
                                                        enabled:
                                                            description: "Enable license"
                                                            type: boolean
                                                            default: false
            responses:
                "204":
                    description: "License Updated successfully."
                "400":
                    description: "Invalid request."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_400"
                "401":
                    description: "Unauthorized access."
                    content:
                        text/html:
                            schema:
                                type: string
                "403":
                    description: "Forbidden access."
                    content:
                        text/html:
                            schema:
                                type: string
                "500":
                    description: "Unexpected error"
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_500"

    /auth/token/revoke:
        post:
            tags:
                - Authentication
            summary: "Revoke auth bearer token"
            operationId: revoke-token
            security:
                - Security_Bearer_Token: []
                - Security_Cookie: []
            x-thoughtspot-public-api-release-version:
                - 9.0.0.cl
            x-thoughtspot-private-api-release-version:
                - 9.0.0.cl
            requestBody:
                description: "Revoke token details"
                content:
                    application/x-www-form-urlencoded:
                        schema:
                            type: object
                            properties:
                                user_identifier:
                                    description: "Unique ID or name of the user whose token needs to be invoked. If not specified then current logged in user identifier is used."
                                    type: string
                                token:
                                    description: "Token which needs to be revoked. If not specified then token from Authorization header is used if available."
                                    type: string
            responses:
                "204":
                    description: "Token successfully revoked."
                "400":
                    description: "Invalid request."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_400"
                "401":
                    description: "Unauthorized access."
                    content:
                        text/html:
                            schema:
                                type: string
                "403":
                    description: "Forbidden access."
                    content:
                        text/html:
                            schema:
                                type: string
                "500":
                    description: "Unexpected error"
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_500"

    /auth/token/validate:
        post:
            tags:
                - Authentication
            summary: "Validate auth bearer token"
            operationId: validate-token
            security:
                - Security_Bearer_Token: [ ]
                - Security_Cookie: [ ]
            x-thoughtspot-public-api-release-version:
                - 9.12.0.cl
            x-thoughtspot-private-api-release-version:
                - 9.12.0.cl
            requestBody:
                description: "Validate token details"
                content:
                    application/x-www-form-urlencoded:
                        schema:
                            type: object
                            required:
                                - token
                            properties:
                                token:
                                    description: "Token which needs to be verified."
                                    type: string
            responses:
                "200":
                    description: "Token validation successful."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Response_Validate_Auth_Token_Data"
                "400":
                    description: "Invalid request."
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_400"
                "401":
                    description: "Unauthorized access."
                    content:
                        text/html:
                            schema:
                                type: string
                "403":
                    description: "Forbidden access."
                    content:
                        text/html:
                            schema:
                                type: string
                "500":
                    description: "Unexpected error"
                    content:
                        application/json:
                            schema:
                                $ref: "#/components/schemas/Error_500"

