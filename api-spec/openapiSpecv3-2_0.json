        Mutate_Column_Security_Rule_Request:
            type: object
            description: "Request object for creating, updating, or deleting column security rules"
            properties:
                identifier:
                    type: string
                    description: "GUID or name of the table for which we want to create column security rules"
                    example: "table-guid"
                obj_identifier:
                    type: string
                    description: "The object ID of the table"
                    example: "table-obj-id"
                clear_csr:
                    type: boolean
                    description: "If clear_csr is true, then all the secured columns will be marked as unprotected, and all the group associations will be removed"
                    default: false
                    example: false
                column_security_rules:
                    type: array
                    description: "An array where each object defines the security rule for a specific column"
                    items:
                        type: object
                        required:
                            - column_identifier
                        properties:
                            column_identifier:
                                type: string
                                description: "Column identifier (col_id or name)"
                                example: "column_123"
                            is_unsecured:
                                type: boolean
                                description: "If is_unsecured is set to true, then the column will be marked as unprotected and all the groups associated with it will be removed"
                                default: false
                                example: false
                            group_access:
                                type: array
                                description: "An array of group operation objects that specifies the actions for a group that needs to be associated with a column"
                                items:
                                    type: object
                                    required:
                                        - operation
                                        - group_identifiers
                                    properties:
                                        operation:
                                            type: string
                                            description: "An enum which will specify the type of operation to be performed on the groups"
                                            enum:
                                                - ADD
                                                - REMOVE
                                                - REPLACE
                                            example: "ADD"
                                        group_identifiers:
                                            type: array
                                            description: "An array of group identifiers (name or GUID) on which the operation will be performed"
                                            items:
                                                type: string
                                            example: [ "group1", "group2", "group3" ]
            required:
                - column_security_rules


        Column_Security_Rule_Response_Data:
            type: object
            required:
                - data
            properties:
                data:
                    type: array
                    items:
                        $ref: "#/components/schemas/Column_Security_Rule_Response"

        Column_Security_Rule_Response:
            type: object
            properties:
                guid:
                    type: string
                    description: "GUID of the table for which the column security rules are fetched"
                    example: "table-guid"
                obj_id:
                    type: string
                    description: "Object ID of the table for which the column security rules are fetched"
                    example: "table-object-id"
                column_security_rules:
                    type: array
                    description: "Array containing column security rule objects"
                    items:
                        $ref: "#/components/schemas/Column_Security_Rule"

        Column_Security_Rule:
            type: object
            properties:
                column:
                    type: object
                    description: "Information about the column"
                    required:
                        - id
                        - name
                    properties:
                        id:
                            type: string
                            description: "The unique identifier of the column"
                            example: "col_123"
                        name:
                            type: string
                            description: "The name of the column"
                            example: "Column Name"
                groups:
                    type: array
                    description: "Array of groups that have access to this column"
                    items:
                        type: object
                        required:
                            - id
                            - name
                        properties:
                            id:
                                type: string
                                description: "The unique identifier of the group"
                                example: "id of the group"
                            name:
                                type: string
                                description: "The name of the group"
                                example: "Admins"
                source_table_details:
                    type: object
                    description: "Information about the source table"
                    required:
                        - id
                        - name
                    properties:
                        id:
                            type: string
                            description: "The unique identifier of the source table"
                            example: "GUID"
                        name:
                            type: string
                            description: "The name of the source table"
                            example: "productcategory"

