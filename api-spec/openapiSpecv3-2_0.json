                                                description: "customObjectId of the metadata."
                                                type: string
                                            type:
                                                description: "Type of metadata."
                                                allOf:
                                                    - $ref: "#/components/schemas/Search_Metadata_Type"
                                            subtypes:
                                                x-thoughtspot-public-api-release-version:
                                                    - 10.11.0.cl
                                                x-thoughtspot-private-api-release-version:
                                                    - 10.11.0.cl
                                                description: "List of subtypes exclusively applicable to LOGICAL_TABLE metadata (ignored for any other type)"
                                                $ref: "#/components/schemas/Search_Metadata_SubType"
                                            name_pattern:
                                                description: "A pattern to match case-insensitive name of the metadata object. User % for a wildcard match."
                                                type: string
                                version:
                                    description: "Version of the metadata object."
                                    type: integer
                                    default: -1
                                dependent_object_version:
                                    description: "Version of the dependent metadata object."
                                    allOf:
                                        - $ref: "#/components/schemas/Metadata_Object_Version"
