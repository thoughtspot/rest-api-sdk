                    example: "X-Webhook-Signature"
                algorithm:
                    type: string
                    description: "Hash algorithm"
                    enum:
                        - SHA256
                    example: "SHA256"
                secret:
                    type: string
                    description: "Shared secret for HMAC"
                    example: "secret_key_123"

        WebhookStorageConfiguration:
            type: object
            description: Configuration for customer-managed storage backend
            required:
                - storage_type
                - storage_config
            properties:
                storage_type:
                    type: string
                    description: Type of storage backend
                    enum:
                        - "AWS_S3"
                    example: "AWS_S3"
                storage_config:
                    $ref: '#/components/schemas/StorageConfig'

        StorageConfig:
            type: object
            description: Storage backend specific configuration. Provide the config for the selected storageType.
            properties:
                aws_s3_config:
                    $ref: '#/components/schemas/AwsS3Config'

        AwsS3Config:
            type: object
            description: AWS S3 configuration for customer-managed storage using STS AssumeRole
            required:
                - bucket_name
                - region
                - role_arn
            properties:
                bucket_name:
                    type: string
                    description: Customer's S3 bucket name where webhook files will be written
                    example: "my-webhook-files"
                region:
                    type: string
                    description: AWS region where the bucket is located
                    example: "us-west-2"
                role_arn:
                    type: string
                    description: |
                        IAM Role ARN in customer's account that ThoughtSpot will assume.
                        Format: arn:aws:iam::<account-id>:role/<role-name>
                    example: "arn:aws:iam::123456789012:role/ThoughtSpotDeliveryRole"
                external_id:
                    type: string
                    description: |
                        External ID for confused deputy protection.
                        - For AWS clusters: Used as External ID in sts:AssumeRole
                        - For GCP clusters: Used as Audience in sts:AssumeRoleWithWebIdentity
                        Customer generates this and includes it in their IAM role trust policy.
                    example: "ts-webhook-a1b2c3d4-e5f6-7890"
                path_prefix:
                    type: string
                    description: |
                        Optional path prefix for organizing files within the bucket.
                        Files will be organized as: {path_prefix}/{clusterId}/{userGuid|external/hashedEmail}/{orgId}/liveboard/{filename}
                    example: "thoughtspot-webhooks/"

        # Individual Authentication Type Components
        Webhook_Auth_API_Key:
            type: object
            title: "API Key Authentication"
            description: "API key authentication with key-value pair."
            required:
                - API_KEY
            properties:
                API_KEY:
                    description: "With API key auth, you send a key-value pair to the API either in the request headers or query parameters."
                    type: object
                    required:
                        - key
                        - value
                    properties:
                        key:
                            description: "Enter your key name"
                            type: string
                            example: "X-API-Key"
                        value:
                            description: "Enter your key value"
                            type: string
                            example: "api_key_123"

        Webhook_Auth_Basic_Auth:
            type: object
            title: "Basic Authentication"
            description: "Basic authentication with username and password."
            required:
                - BASIC_AUTH
            properties:
                BASIC_AUTH:
                    description: "Basic Auth: Basic authentication involves sending a verified username and password with your request."
                    type: object
                    required:
                        - username
                        - password
                    properties:
                        username:
                            description: "Username for the basic authentication"
                            type: string
                            example: "user123"
                        password:
                            description: "Password for the basic authentication"
                            type: string
                            example: "password123"

        Webhook_Auth_Bearer_Token:
            type: object
            title: "Bearer Token Authentication"
            description: "Bearer token authentication."
            required:
                - BEARER_TOKEN
            properties:
                BEARER_TOKEN:
                    description: "Bearer tokens enable requests to authenticate using an access key."
                    type: string
                    example: "bearer_token_123"

        Webhook_Auth_OAuth2:
            type: object
            title: "OAuth2 Authentication"
            description: "OAuth 2.0 authentication using client credentials flow."
            required:
                - OAUTH2
            properties:
                OAUTH2:
                    description: "OAuth 2.0 authentication using client credentials flow."
                    type: object
                    required:
                        - client_id
                        - client_secret
                        - authorization_url
                    properties:
                        client_id:
                            description: "OAuth 2.0 client identifier"
                            type: string
                            example: "client_123"
                        client_secret:
                            description: "OAuth 2.0 client secret key"
                            type: string
                            example: "secret_key_456"
                        authorization_url:
                            description: "OAuth 2.0 authorization server URL"
                            type: string
                            format: uri
                            example: "https://auth.example.com/oauth2/authorize"

        Webhook_Authentication:
            description: "Authorization Configurations for different Authorization Types. Only ONE authentication type should be specified."
            oneOf:
                - $ref: "#/components/schemas/Webhook_Auth_API_Key"
                - $ref: "#/components/schemas/Webhook_Auth_Basic_Auth"
                - $ref: "#/components/schemas/Webhook_Auth_Bearer_Token"
                - $ref: "#/components/schemas/Webhook_Auth_OAuth2"

        Error_400:
            type: object
            required:
                - error
            properties:
                error:
                    type: object
                    required:
                        - message
                    properties:
                        message:
                            type: object
                            required:
                                - code
                                - incident_id_guid
                                - debug
                            properties:
                                code:
                                    type: integer
                                incident_id_guid:
                                    type: string
                                trace_id_guid:
                                    type: string
                                debug:
                                    type: string

        Error_500:
            type: object
            required:
                - error
            properties:
                error:
                    type: object
                    required:
                        - message
                    properties:
                        message:
                            type: object
                            required:
                                - code
                                - incident_id_guid
                                - debug
                            properties:
                                code:
                                    type: integer
                                incident_id_guid:
                                    type: string
                                trace_id_guid:
                                    type: string
                                debug:
                                    type: string

    # Supported security schema definitions
    securitySchemes:
        #Security_Basic_Auth:
        #    description: "Basic Auth"
        #    type: http
        #    scheme: basic
        #Security_Token_Auth:
        #    description: 'Custom Bearer Token Auth'
        #    type: apiKey
        #    in: header
        #    name: Authorization
        Security_Bearer_Token:
            description: "Bearer Token Auth"
            type: http
            scheme: bearer
            bearerFormat: JWT
        Security_Cookie:
            description: "Cookie Based Auth"
            type: apiKey
            in: cookie
            name: JSESSIONID

# Default applicable security schemas for all API's
security:
    - Security_Bearer_Token: []
    - Security_Cookie: []
